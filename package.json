{
    "name": "@egomobile/nats",
    "version": "0.4.0",
    "description": "Classes, functions and tools that help to connect and communicate to and with a NATS streaming server.",
    "main": "lib/index.js",
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "del ./lib && tsc",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
        "sandbox": "nodemon --watch './sandbox.ts' --exec node -r ts-node/register --inspect=0.0.0.0:9229 ./sandbox.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/egomobile/node-nats.git"
    },
    "keywords": [
        "node",
        "nats",
        "streaming",
        "client",
        "listener",
        "publisher",
        "classes",
        "tools",
        "functions",
        "typescript",
        "js",
        "ts",
        "javascript",
        "typescript",
        "ego"
    ],
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/*.map",
        "README.md"
    ],
    "dependencies": {
        "@types/node": "12.20.33",
        "node-nats-streaming": "0.3.2",
        "p-queue": "6.6.2"
    },
    "devDependencies": {
        "@egomobile/tsconfig": "5.0.0",
        "del-cli": "4.0.1",
        "eslint": "7.32.0",
        "eslint-config-ego": "0.15.0",
        "nodemon": "2.0.13",
        "ts-node": "10.3.0",
        "tslint": "6.1.3",
        "typedoc": "0.22.6",
        "typescript": "4.4.4"
    }
}